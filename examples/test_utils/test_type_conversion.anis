// Test Type Conversion Built-ins
println("=== Type Conversion Tests ===");

// Number() Tests
println("\n--- Number() Tests ---");
println("Number(true) = " + Number(true));           // Should be 1
println("Number(false) = " + Number(false));         // Should be 0
println("Number('123') = " + Number("123"));         // Should be 123
println("Number('abc') = " + Number("abc"));         // Should be 0 (NaN)
println("Number(456) = " + Number(456));             // Should be 456
println("Number() = " + Number());                   // Should be 0

// String() Tests
println("\n--- String() Tests ---");
println("String(123) = '" + String(123) + "'");      // Should be "123"
println("String(true) = '" + String(true) + "'");    // Should be "true"
println("String(false) = '" + String(false) + "'");  // Should be "false"
println("String('hello') = '" + String("hello") + "'"); // Should be "hello"

// Boolean() Tests
println("\n--- Boolean() Tests ---");
println("Boolean(0) = " + Boolean(0));               // Should be false
println("Boolean(1) = " + Boolean(1));               // Should be true
println("Boolean(5) = " + Boolean(5));               // Should be true
println("Boolean('') = " + Boolean(""));             // Should be false
println("Boolean('a') = " + Boolean("a"));           // Should be true
println("Boolean('false') = " + Boolean("false"));   // Should be false
println("Boolean('0') = " + Boolean("0"));           // Should be false
println("Boolean(true) = " + Boolean(true));         // Should be true

// Practical Examples
println("\n--- Practical Examples ---");
var count = "42";
println("String '42' to number: " + Number(count));

var isActive = 1;
println("Number 1 to boolean: " + Boolean(isActive));

var userId = 123;
println("Number 123 to string: '" + String(userId) + "'");

println("\n=== All Tests Complete ===");
